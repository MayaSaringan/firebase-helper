{
  "name": "firebase-helper",
  "version": "0.0.1",
  "description": "Wraps around the firebase API for Client in the exact fashion I usually use in my projects",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
